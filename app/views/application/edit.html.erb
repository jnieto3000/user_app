<h2>Edit User</h2>

<form action="/update_user/<%= @user.id %>">
  <input type="text" name="image_url" placeholder="image_url" value="<%= @user.image_url %>" >
  <input type="text" name="user_name" placeholder="user_name" value="<%= @user.name %>">
  <input type="string" name="bio" placeholder="bio" value="<%= @user.bio %>">
  <input type="submit">
  
</form>